<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="login_text_label_style">
        <item name="android:layout_marginTop">@dimen/_20ssp</item>
        <item name="android:layout_marginEnd">@dimen/_30ssp</item>
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textColor">#544D4030</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="consultation_text_label_style">
    <item name="android:layout_marginTop">@dimen/_5ssp</item>
    <item name="android:layout_marginEnd">@dimen/_30ssp</item>
    <item name="android:fontFamily">@font/grotesk_pro_light</item>
    <item name="android:textColor">@color/brown_new</item>
    <item name="android:textSize">12sp</item>
    </style>

    <style name="login_edit_text_style">
        <item name="android:backgroundTint">#4D827564</item>
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textCursorDrawable">@drawable/cursor</item>
    </style>

    <style name="button_style">
        <item name="android:layout_marginStart">@dimen/_15ssp</item>
        <item name="android:layout_marginEnd">@dimen/_15ssp</item>
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">@dimen/_5ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>


    <style name="button_style_l">
        <item name="android:layout_marginStart">@dimen/_15ssp</item>
        <item name="android:layout_marginEnd">@dimen/_15ssp</item>
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">@dimen/_5ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:background">@drawable/drawable_bg_with_border</item>
        <item name="android:backgroundTint">@color/app_color_light_new</item>
    </style>


    <style name="login_button_style">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">@dimen/_5ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="counsultaion_button_style">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="button_style_full">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">@dimen/_5ssp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="style_error_tv">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:layout_marginStart">@dimen/_3sdp</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="android:layout_marginTop">-3dp</item>
        <item name="android:textColor">@color/red_color</item>
        <item name="android:visibility">gone</item>
    </style>


    <style name="TextAppearance.MaterialCalendarWidget.Header" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.MaterialCalendarWidget.Date" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/mcv_text_date_light</item>

    </style>

    <style name="TextAppearance.MaterialCalendarWidget.WeekDay" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_marginStart">@dimen/_50sdp</item>
        <item name="android:layout_marginEnd">@dimen/_50sdp</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="bottomsheet_label_style">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">10sp</item>
        <item name="android:layout_marginTop">@dimen/_3ssp</item>
        <item name="android:textColor">@color/grey_30</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="bottomsheet_label_style_potrate">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">@dimen/_3ssp</item>
        <item name="android:textColor">@color/grey_30</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <style name="bottomSheet_label_style_landscape">
        <item name="android:fontFamily">@font/grotesk_pro_light</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_marginTop">@dimen/_3ssp</item>
        <item name="android:textColor">@color/grey_30</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="PopupMenu">
        <item name="android:itemBackground">@color/black_40</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:colorBackground">@color/black_40</item>
        <item name="android:dividerHeight">1dp</item>
    </style>

    <style name="SelectableItemTheme">
        <item name="colorControlHighlight">@color/brown_new</item>
    </style>

    <style name="SelectableItemBackground">
        <item name="android:theme">@style/SelectableItemTheme</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:textColor">@color/white</item>
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">@color/brown_new</item>
    </style>

</resources>